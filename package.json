{
  "name": "smartbirds-server",
  "version": "1.3.2",
  "description": "SmartBirds Server",
  "author": "Geno Roupsky",
  "private": true,
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-environment-ah": "file:./__utils__/jest-environment-ah",
    "jest-runner-mocha": "^0.6.0",
    "lint-staged": "^10.2.11",
    "parallelshell": "^3.0.2",
    "sequelize-fixtures": "^1.2.0",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^9.0.2",
    "sqlite3": "^5.0.0",
    "standard": "^14.3.4"
  },
  "scripts": {
    "commit": "git-cz",
    "help": "npm run ah -- help",
    "start": "npm run ah -- start cluster",
    "ah": "/usr/bin/env PROJECT_ROOT=`pwd`/server node --max_old_space_size=2048 node_modules/.bin/actionhero",
    "sq": "sequelize",
    "jest": "NODE_ENV=test PROJECT_ROOT=`pwd`/server jest",
    "less": "lessc",
    "pretest": "standard",
    "test": "npm run test:sqlite && npm run test:pg",
    "test:sqlite": "TEST_DATABASE_URL='' npm run jest --",
    "test:pg": "TEST_DATABASE_URL=${TEST_DATABASE_URL:=postgres://testuser:testpass@localhost:5434/testdb} npm run jest -- --config jest.pg.config.js",
    "test:watch": "npm run jest -- --watch",
    "dev": "docker-compose start",
    "prestart": "npm run migrate",
    "migrate": "npm run sq -- db:migrate",
    "seed": "npm run sq -- db:seed",
    "enqueue": "npm run ah -- task enqueue --name",
    "heroku-postbuild": "(echo \"Promise.all(['organizations:export', 'stats:generate', 'banner:generate'].map(t => api.tasks.tasks[t].run(api, {}))).then(() => console.log('DONE'), (e) => console.error('BAD', e))\";sleep 10) | npm run ah console"
  },
  "dependencies": {
    "@sentry/node": "^5.20.0",
    "actionhero": "^20.0.0",
    "archiver": "^4.0.2",
    "bcrypt": "^5.0.0",
    "bluebird": "~3.7.2",
    "blueimp-md5": "^2.16.0",
    "boolean": "^3.0.1",
    "concat-stream": "^2.0.0",
    "content-addressable-blob-store": "^5.1.0",
    "csv-parse": "~4.11.1",
    "csv-stringify": "^5.5.0",
    "date-fns": "^2.14.0",
    "debug": "^4.1.1",
    "ejs": "^3.1.3",
    "email-templates": "^7.1.0",
    "form-data": "^3.0.0",
    "geolib": "^3.3.1",
    "ioredis": "^4.17.3",
    "lodash": "^4.17.19",
    "mailchimp-api-v3": "^1.14.0",
    "mime-types": "^2.1.27",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.0",
    "nodemailer-mailgun-transport": "^2.0.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.2.0",
    "sharp": "^0.25.4",
    "umzug": "~2.3.0",
    "uuid": "^8.2.0",
    "validator": "~13.1.1"
  },
  "standard": {
    "ignore": [
      "**/test/",
      "/public/"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
