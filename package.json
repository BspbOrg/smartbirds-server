{
  "name": "smartbirds-server",
  "version": "1.25.0",
  "description": "SmartBirds Server",
  "author": "Geno Roupsky",
  "private": true,
  "engines": {
    "node": "22"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-runner-mocha": "^0.6.0",
    "lint-staged": "^16.2.6",
    "parallelshell": "^3.0.2",
    "sequelize-fixtures": "^1.2.0",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^21.0.0",
    "sqlite3": "^5.1.7",
    "standard": "^17.1.2"
  },
  "scripts": {
    "deploy": "yarn version && git push && git push --tags && while ! git push origin HEAD:production; do sleep 1m; done",
    "commit": "git-cz",
    "help": "npm run ah -- help",
    "start": "npm run ah -- start",
    "ah": "/usr/bin/env PROJECT_ROOT=`pwd`/server node --max_old_space_size=2048 node_modules/.bin/actionhero",
    "sq": "sequelize",
    "jest": "NODE_ENV=test PROJECT_ROOT=`pwd`/server node --require ./suppress-dep0174.js node_modules/.bin/jest",
    "less": "lessc",
    "pretest": "standard",
    "test": "npm run test:sqlite && npm run test:pg",
    "test:sqlite": "TEST_DATABASE_URL='' npm run jest --",
    "test:pg": "TEST_DATABASE_URL=${TEST_DATABASE_URL:=postgres://testuser:testpass@localhost:5434/testdb} npm run jest -- --config jest.pg.config.js",
    "test:dbschema": "DB_SCHEMA=y npm run test:pg -- db-schema",
    "test:watch": "npm run jest -- --watch",
    "dev": "docker-compose start",
    "prestart": "npm run migrate",
    "migrate": "npm run ah -- db migrate",
    "seed": "npm run sq -- db:seed",
    "enqueue": "npm run ah -- task enqueue --name",
    "heroku-postbuild": "npm run ah -- db migrate;(echo \"Promise.all(['organizations:export', 'stats:generate', 'banner:generate'].map(t => api.tasks.tasks[t].run(api, {}))).then(() => console.log('DONE'), (e) => console.error('BAD', e))\";sleep 10) | npm run ah console",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@sentry/node": "^10.24.0",
    "actionhero": "^20.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bluebird": "~3.7.2",
    "blueimp-md5": "^2.19.0",
    "boolean": "^3.2.0",
    "concat-stream": "^2.0.0",
    "content-addressable-blob-store": "^5.1.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "debug": "^4.3.6",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "email-templates": "^12.0.3",
    "form-data": "^4.0.0",
    "geolib": "^3.3.4",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "mailchimp-api-v3": "^1.15.0",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "node-fetch": "^2.7.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.6.3",
    "sharp": "^0.34.5",
    "umzug": "^3.8.2",
    "uuid": "^13.0.0",
    "validator": "^13.15.23"
  },
  "standard": {
    "ignore": [
      "**/test/",
      "/public/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
