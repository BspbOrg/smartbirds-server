{
  "name": "smartbirds-server",
  "version": "1.13.0",
  "description": "SmartBirds Server",
  "author": "Geno Roupsky",
  "private": true,
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^27.4.3",
    "jest-runner-mocha": "^0.6.0",
    "lint-staged": "^11.2.6",
    "parallelshell": "^3.0.2",
    "sequelize-fixtures": "^1.2.0",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^12.0.1",
    "sqlite3": "^5.0.2",
    "standard": "^16.0.4"
  },
  "scripts": {
    "commit": "git-cz",
    "help": "npm run ah -- help",
    "start": "npm run ah -- start cluster",
    "ah": "/usr/bin/env PROJECT_ROOT=`pwd`/server node --max_old_space_size=2048 node_modules/.bin/actionhero",
    "sq": "sequelize",
    "jest": "NODE_ENV=test PROJECT_ROOT=`pwd`/server jest",
    "less": "lessc",
    "pretest": "standard",
    "test": "npm run test:sqlite && npm run test:pg",
    "test:sqlite": "TEST_DATABASE_URL='' npm run jest --",
    "test:pg": "TEST_DATABASE_URL=${TEST_DATABASE_URL:=postgres://testuser:testpass@localhost:5434/testdb} npm run jest -- --config jest.pg.config.js",
    "test:watch": "npm run jest -- --watch",
    "dev": "docker-compose start",
    "prestart": "npm run migrate",
    "migrate": "npm run sq -- db:migrate",
    "seed": "npm run sq -- db:seed",
    "enqueue": "npm run ah -- task enqueue --name",
    "heroku-postbuild": "(echo \"Promise.all(['organizations:export', 'stats:generate', 'banner:generate'].map(t => api.tasks.tasks[t].run(api, {}))).then(() => console.log('DONE'), (e) => console.error('BAD', e))\";sleep 10) | npm run ah console",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/node": "^6.13.3",
    "actionhero": "^20.0.0",
    "archiver": "^5.3.0",
    "bcrypt": "^5.0.1",
    "bluebird": "~3.7.2",
    "blueimp-md5": "^2.19.0",
    "boolean": "^3.1.4",
    "concat-stream": "^2.0.0",
    "content-addressable-blob-store": "^5.1.0",
    "csv-parse": "~4.16.3",
    "csv-stringify": "^6.0.4",
    "date-fns": "^2.27.0",
    "debug": "^4.3.3",
    "ejs": "^3.1.6",
    "email-templates": "^8.0.8",
    "form-data": "^4.0.0",
    "geolib": "^3.3.3",
    "husky": "^7.0.4",
    "ioredis": "^4.28.2",
    "lodash": "^4.17.21",
    "mailchimp-api-v3": "^1.15.0",
    "mime-types": "^2.1.34",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "node-fetch": "^2.6.1",
    "nodemailer-mailgun-transport": "^2.1.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.3.0",
    "sharp": "^0.29.2",
    "umzug": "~2.3.0",
    "uuid": "^8.3.2",
    "validator": "~13.7.0"
  },
  "standard": {
    "ignore": [
      "**/test/",
      "/public/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
