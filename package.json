{
  "name": "smartbirds-server",
  "version": "0.13.2",
  "description": "SmartBirds Server",
  "author": "Geno Roupsky",
  "private": true,
  "engines": {
    "node": "~8"
  },
  "devDependencies": {
    "budo": "^11.5.0",
    "disc": "^1.3.3",
    "escape-string-regexp": "^1.0.5",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "parallelshell": "^3.0.2",
    "sequelize-fixtures": "^0.7.0",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^7.1.1",
    "sqlite3": "^4.0.4",
    "standard": "^12.0.1",
    "watch": "^1.0.2",
    "watchify": "^3.11.0"
  },
  "scripts": {
    "help": "npm run ah -- help",
    "start": "npm run ah -- start cluster",
    "ah": "/usr/bin/env PROJECT_ROOT=`pwd`/server node --max_old_space_size=2048 node_modules/.bin/actionhero",
    "sq": "sequelize",
    "mocha": "NODE_ENV=test PROJECT_ROOT=`pwd`/server mocha --globals config",
    "less": "lessc",
    "pretest": "standard",
    "test": "npm run mocha --",
    "test:watch": "npm run mocha -- --reporter min --watch -G --full-trace",
    "dev": "docker-compose start && parallelshell 'npm run budo' 'npm run build:css:watch'",
    "build:js:debug": "browserify ./client/scripts/app.js -d -o ./public/js/scripts.js",
    "build:js": "cross-env NODE_ENV=${ENV:=production} browserify -g envify --debug -e ./client/scripts/app.js | uglifyjs -c warnings=false -m > ./public/js/scripts.js",
    "discify": "cross-env NODE_ENV=${ENV:=production} browserify --full-paths -g envify --debug -e ./client/scripts/app.js | uglifyjs -c warnings=false -m | discify > ./public/disc.html",
    "build:js:watch": "watchify ./client/scripts/app.js -d -o ./public/js/scripts.js -v",
    "build:css": "lessc --clean-css --source-map --source-map-less-inline client/styles/main.less public/css/main.css",
    "build:css:watch": "watch 'npm run build:css' client/styles",
    "build": "npm run build:css && npm run build:js",
    "postinstall": "bower install --allow-root",
    "prestart": "npm run migrate",
    "migrate": "npm run sq -- db:migrate",
    "seed": "npm run sq -- db:seed",
    "budo": "budo ./client/scripts/app.js --serve js/scripts.js  -d public/ --live --pushstate",
    "enqueue": "npm run ah -- task enqueue --name"
  },
  "dependencies": {
    "actionhero": "^17.1.1",
    "archiver": "^1.1.0",
    "bcrypt": ">=3",
    "bluebird": "~3.0.5",
    "blueimp-md5": "^2.10.0",
    "bower": "^1.8.2",
    "browserify": "~12.0.1",
    "browserify-ngannotate": "^2.0.0",
    "browserify-shim": "~3.8.0",
    "bulk-require": "~0.2.1",
    "bulkify": "~1.1.1",
    "concat-stream": "^1.5.2",
    "content-addressable-blob-store": "^4.6.0",
    "cross-env": "^5.1.1",
    "csv-parse": "~1.0.0",
    "csv-stringify": "^1.1.2",
    "debug": "^2.6.9",
    "ejs": "^2.5.7",
    "email-templates": "^2.7.1",
    "envify": "~3.4.0",
    "fakeredis": "^2.0.0",
    "filestorage": "^1.7.0",
    "form-data": "^2.3.1",
    "ioredis": "^4.9.1",
    "less": "~3.8.1",
    "less-plugin-clean-css": "~1.5.1",
    "localforage": "^1.7.3",
    "lodash": "^4.16.6",
    "mailchimp-api-v3": "^1.8.0",
    "mime-types": "^2.1.17",
    "minifyify": "^7.3.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.19.3",
    "moment-timezone": "^0.5.14",
    "node-fetch": "^1.7.3",
    "nodemailer": "^1.10.0",
    "nodemailer-mailgun-transport": "^1.3.6",
    "pg": "^5.0.0",
    "pg-hstore": "~2.3.2",
    "querystring": "^0.2.0",
    "raven": "^0.10.0",
    "sequelize": "^3.31.1",
    "sequelize-cli": "^3.2.0",
    "sharp": "^0.17.3",
    "simplify-js": "^1.2.1",
    "speakingurl": "^7.0.0",
    "stringify": "^3.2.0",
    "uglify-js": "^3.2.1",
    "umzug": "~1.6.0",
    "underscore.string": "^3.3.4",
    "uuid": "^2.0.3",
    "validator": "~4.2.1",
    "workbox-window": "^4.3.0",
    "ws": "^3.3.2"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-i18n": "./bower_components/angular-i18n/angular-locale_bg.js",
    "ui.bootstrap": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "uiGmapgoogle-maps": "./bower_components/angular-google-maps/dist/angular-google-maps.js",
    "ngSanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "angular-simple-logger": "./bower_components/angular-simple-logger/dist/index.light.js",
