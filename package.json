      ]
    },
    "angular-filter": {
      "depends": [
        "angular:angular"
      ]
    },
    "angulartics": {
      "depends": [
        "angular:angular"
      ]
    },
    "angulartics-ga": {
      "depends": [
        "angulartics:angulartics"
      ]
    },
    "angular-file-upload": {
      "depends": [
        "angular:angular"
      ]
    },
    "ngstorage": {
