    "angular-translate-loader-url": {
      "depends": [
        "angular:angular"
      ]
    }
  },
  "browserify": {
    "transform": [
      "stringify",
      "browserify-shim",
      "bulkify",
      "envify",
      "browserify-ngannotate"
    ]
  },
  "standard": {
    "ignore": [
      "**/test/",
      "/public/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  }
