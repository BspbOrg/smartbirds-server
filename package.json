{
  "name": "smartbirds-server",
  "version": "0.3.1",
  "description": "SmartBirds Server",
  "author": "Geno Roupsky",
  "private": true,
  "devDependencies": {
    "budo": "latest",
    "escape-string-regexp": "^1.0.3",
    "live-reload": "latest",
    "mocha": "latest",
    "parallelshell": "latest",
    "sequelize-fixtures": "latest",
    "should": "latest",
    "should-sinon": "0.0.3",
    "sinon": "^1.17.2",
    "sqlite3": "latest",
    "watch": "latest",
    "watchify": "latest"
  },
  "scripts": {
    "help": "npm run ah -- help",
    "start": "npm run ah -- start",
    "ah": "PROJECT_ROOT=`pwd`/server actionhero",
    "sq": "sequelize",
    "mocha": "NODE_ENV=test PROJECT_ROOT=`pwd`/server mocha --globals config",
    "less": "lessc",
    "test": "npm run mocha --",
    "test:watch": "npm run mocha -- --reporter min --watch -G --full-trace",
    "dev": "docker-compose start && parallelshell 'npm run budo' 'npm run build:css:watch'",
    "build:js": "browserify ./client/scripts/app.js -d -o ./public/js/scripts.js",
    "build:js:watch": "watchify ./client/scripts/app.js -d -o ./public/js/scripts.js -v",
    "build:css": "lessc --clean-css --source-map --source-map-less-inline client/styles/main.less public/css/main.css",
    "build:css:watch": "watch 'npm run build:css' client/styles",
    "build": "npm run build:css && npm run build:js",
    "postinstall": "bower install --allow-root",
    "prestart": "npm run migrate && npm run build",
    "migrate": "npm run sq -- db:migrate",
    "seed": "npm run sq -- db:seed",
    "budo": "budo ./client/scripts/app.js --serve js/scripts.js  -d public/ --live --pushstate"
  },
  "dependencies": {
    "actionhero": "~12.1.2",
    "bcrypt": "~0.8.5",
    "bluebird": "~3.0.5",
    "bower": "~1.7.7",
    "browserify": "~12.0.1",
    "browserify-shim": "~3.8.0",
    "bulk-require": "~0.2.1",
    "bulkify": "~1.1.1",
    "csv-parse": "~1.0.0",
    "csv-stringify": "^1.0.2",
    "debug": "^2.2.0",
    "ejs": "^2.3.4",
    "email-templates": "^2.0.1",
    "envify": "~3.4.0",
    "http-server": "~0.8.5",
    "less": "~2.5.3",
    "less-plugin-clean-css": "~1.5.1",
    "lodash": "~3.10.0",
    "moment": "^2.11.1",
    "nodemailer": "^1.10.0",
    "nodemailer-mailgun-transport": "^1.0.1",
    "pg": "~4.4.3",
    "pg-hstore": "~2.3.2",
    "raven": "^0.10.0",
    "sequelize": "~3.13.0",
    "sequelize-cli": "~2.1.0",
    "speakingurl": "^7.0.0",
    "stringify": "^3.2.0",
    "umzug": "~1.6.0",
    "validator": "~4.2.1"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-i18n": "./bower_components/angular-i18n/angular-locale_bg.js",
    "ui.bootstrap": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "uiGmapgoogle-maps": "./bower_components/angular-google-maps/dist/angular-google-maps.js",
    "ngSanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "angular-simple-logger": "./bower_components/angular-simple-logger/dist/index.js",
    "ui.router": "./bower_components/angular-ui-router/release/angular-ui-router.js",
    "ui.select": "./bower_components/ui-select/dist/select.js",
    "chance": "./bower_components/chance/chance.js",
    "angular-resource": "./bower_components/angular-resource/angular-resource.js",
    "angular-cookies": "./bower_components/angular-cookies/angular-cookies.js",
    "angular-strap": "./bower_components/angular-strap/dist/angular-strap.compat.js",
    "angular-strap-tpl": "./bower_components/angular-strap/dist/angular-strap.tpl.js",
    "nya-bootstrap-select": "./bower_components/nya-bootstrap-select/dist/js/nya-bs-select.js",
    "ngToast": "./bower_components/ngToast/dist/ngToast.js",
    "ngAnimate": "./bower_components/angular-animate/angular-animate.js",
    "ngInfiniteScroll": "./bower_components/ngInfiniteScroll/build/ng-infinite-scroll.js",
    "debug": "./node_modules/debug/browser.js",
    "angular-loading-bar": "./bower_components/angular-loading-bar/build/loading-bar.js",
    "raven-js": "./bower_components/raven-js/dist/raven.js",
    "raven-js-angular": "./bower_components/raven-js/dist/plugins/angular.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "raven-js": "Raven",
    "raven-js-angular": {
      "depends": [
        "raven-js:Raven"
      ]
    },
    "angular-i18n": {
      "depends": [
        "angular:angular"
      ]
    },
    "ui.bootstrap": {
      "depends": [
        "angular:angular"
      ]
    },
    "uiGmapgoogle-maps": {
      "depends": [
        "angular:angular",
        "angular-simple-logger",
        "lodash:_"
      ]
    },
    "ngSanitize": {
      "depends": [
        "angular:angular"
      ]
    },
    "ui.router": {
      "depends": [
        "angular:angular"
      ]
    },
    "ui.select": {
      "depends": [
        "angular:angular"
      ]
    },
    "chance": "chance",
    "lodash": "_",
    "angular-resource": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular-cookies": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular-strap": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular-strap-tpl": {
      "depends": [
        "angular:angular"
      ]
    },
    "nya-bootstrap-select": {
      "depends": [
        "angular:angular"
      ]
    },
    "ngToast": {
      "depends": [
        "angular:angular"
      ]
    },
    "ngAnimate": {
      "depends": [
        "angular:angular"
      ]
    },
    "ngInfiniteScroll": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular-loading-bar": {
      "depends": [
        "angular:angular"
      ]
    }
  },
  "browserify": {
    "transform": [
      "stringify",
      "browserify-shim",
      "bulkify",
      "envify"
    ]
  }
}
