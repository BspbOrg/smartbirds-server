    "ngAnimate": "./bower_components/angular-animate/angular-animate.js",
    "ngInfiniteScroll": "./bower_components/ngInfiniteScroll/build/ng-infinite-scroll.js",
    "angular-loading-bar": "./bower_components/angular-loading-bar/build/loading-bar.js",
    "raven-js": "./bower_components/raven-js/dist/raven.js",
    "raven-js-angular": "./bower_components/raven-js/dist/plugins/angular.js",
    "angular-filter": "./bower_components/angular-filter/dist/angular-filter.js",
    "angulartics": "./bower_components/angulartics/src/angulartics.js",
    "angulartics-ga": "./bower_components/angulartics-google-analytics/lib/angulartics-ga.js",
    "angular-file-upload": "./bower_components/angular-file-upload/dist/angular-file-upload.js",
    "ngstorage": "./bower_components/ngstorage/ngStorage.js",
    "x2js": "./bower_components/x2js/xml2json.js",
    "angular-xml": "./bower_components/angular-xml/angular-xml.js",
    "angular-bootstrap-lightbox": "./bower_components/angular-bootstrap-lightbox/dist/angular-bootstrap-lightbox.js",
    "debug": "./node_modules/debug/src/debug.js",
    "angular-translate": "./bower_components/angular-translate/angular-translate.js",
    "angular-translate-loader-url": "./bower_components/angular-translate-loader-url/angular-translate-loader-url.js",
    "odometer": "./bower_components/odometer/odometer.js"
  },
  "browserify-shim": {
