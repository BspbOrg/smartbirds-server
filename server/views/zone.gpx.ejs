<?xml version="1.0" encoding="UTF-8"?>
<gpx xmlns="http://www.topografix.com/GPX/1/1"
     version="1.1"
     creator="SmartBirds Server"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.topografix.com/GPX/1/1 http://www.topografix.com/GPX/1/1/gpx.xsd">
  <metadata>
    <time><%=zone.updatedAt%></time>
  </metadata>
  <trk>
    <name><%=zone.id%></name>
    <trkseg>
      <trkpt lat="<%=zone.lat1%>" lon="<%=zone.lon1%>" />
      <trkpt lat="<%=zone.lat2%>" lon="<%=zone.lon2%>" />
      <trkpt lat="<%=zone.lat3%>" lon="<%=zone.lon3%>" />
      <trkpt lat="<%=zone.lat4%>" lon="<%=zone.lon4%>" />
      <trkpt lat="<%=zone.lat1%>" lon="<%=zone.lon1%>" />
    </trkseg>
  </trk>
</gpx>
